{
  "extends": "airbnb-base",
  "parser": "babel-eslint",
  "parserOptions":{
    "ecmaFeatures": {
      "classes": true,
      "experimentalObjectRestSpread": true
    }
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "new-cap": 1,
    "padded-blocks": 1,
    "max-len": 1,
    "camelcase": 1,
  }
}
