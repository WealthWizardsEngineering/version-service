{
  "name": "version-service",
  "version": "1.0.25",
  "engines": {
    "node": "^6.14.2"
  },
  "description": "A service to store historic versions of applications",
  "homepage": "https://github.com/WealthWizardsEngineering/version-service",
  "repository": {
    "type": "git",
    "url": "https://github.com/WealthWizardsEngineering/version-service.git"
  },
  "main": "main.js",
  "scripts": {
    "start": "node main.js",
    "test:unit": "tape 'test/unit/**/*.spec.js'",
    "test:unit:coverage": "nyc --report-dir=coverage/unit --reporter=text --reporter=html --reporter=lcov npm run test:unit",
    "test:component": "tape 'test/component/**/*.spec.js'",
    "lint": "eslint *.js",
    "lint:fix": "eslint --fix *.js",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "author": "Wealth Wizards",
  "license": "MIT",
  "devDependencies": {
    "babel-eslint": "10.0.2",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-0": "6.24.1",
    "eslint": "6.8.0",
    "eslint-config-airbnb-base": "13.2.0",
    "eslint-plugin-import": "2.22.1",
    "node-mocks-http": "1.7.6",
    "nyc": "15.0.0",
    "proxyquire": "2.1.1",
    "sinon": "7.4.1",
    "supertest": "4.0.2",
    "tape": "4.11.0",
    "timekeeper": "2.2.0"
  },
  "dependencies": {
    "body-parser": "1.19.0",
    "bunyan": "1.8.13",
    "co": "4.6.0",
    "cors": "2.8.5",
    "dotenv": "8.0.0",
    "envalid": "5.0.0",
    "express": "4.17.1",
    "gridfs-stream": "1.1.1",
    "helmet": "3.21.1",
    "joi": "14.3.1",
    "lodash": "4.17.20",
    "mongoose": "5.10.13",
    "nocache": "2.1.0",
    "response-time": "2.3.2",
    "snyk": "^1.189.0"
  },
  "snyk": true
}
