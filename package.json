{
  "name": "version-service",
  "version": "1.0.8",
  "engines": {
    "node": "^6.14.2"
  },
  "description": "A service to store historic versions of applications",
  "homepage": "https://github.com/WealthWizardsEngineering/version-service",
  "repository": {
    "type": "git",
    "url": "https://github.com/WealthWizardsEngineering/version-service.git"
  },
  "main": "main.js",
  "scripts": {
    "start": "node main.js",
    "test:unit": "tape 'test/unit/**/*.spec.js'",
    "test:unit:coverage": "nyc --report-dir=coverage/unit --reporter=text --reporter=html --reporter=lcov npm run test:unit",
    "test:component": "tape 'test/component/**/*.spec.js'",
    "lint": "eslint *.js",
    "lint:fix": "eslint --fix *.js",
    "dependency-check": "nsp check"
  },
  "author": "Wealth Wizards",
  "license": "MIT",
  "devDependencies": {
    "babel-eslint": "8.2.3",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-0": "6.24.1",
    "eslint": "4.19.1",
    "eslint-config-airbnb-base": "12.1.0",
    "eslint-plugin-import": "2.12.0",
    "node-mocks-http": "1.5.8",
    "nsp": "3.2.1",
    "nyc": "11.8.0",
    "proxyquire": "2.0.1",
    "sinon": "5.0.7",
    "supertest": "3.1.0",
    "tape": "4.9.0",
    "timekeeper": "2.1.2"
  },
  "dependencies": {
    "body-parser": "1.18.3",
    "bunyan": "1.8.12",
    "co": "4.6.0",
    "cors": "2.8.4",
    "dotenv": "5.0.1",
    "envalid": "4.1.4",
    "express": "4.16.3",
    "gridfs-stream": "1.1.1",
    "helmet": "3.12.1",
    "joi": "13.3.0",
    "lodash": "4.17.10",
    "mongoose": "5.1.1",
    "nocache": "2.0.0",
    "response-time": "2.3.2"
  }
}
