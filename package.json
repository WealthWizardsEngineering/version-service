{
  "name": "version-service",
  "version": "0.0.1",
  "engines": {
    "node": "6.2.0"
  },
  "description": "A service to retrieve application versions",
  "main": "main.js",
  "scripts": {
    "start": "node main.js",
    "test:unit": "tape 'src/**/*.spec.js'",
    "test:unit:watch": "nodemon -q -x 'npm run test:unit | tap-spec --silent'",
    "test:unit:coverage": "nyc --report-dir=coverage/unit --reporter=text --reporter=html npm run test:unit",
    "test:unit:report": "npm run test:unit:coverage | tee /dev/stderr | node_modules/tap-xunit/bin/tap-xunit > unit-test-report.xml",
    "test:component": "tape 'test/component/**/*.spec.js'",
    "test:component:coverage": "nyc --report-dir=coverage/component --reporter=text --reporter=html npm run test:component",
    "test:component:report": "npm run test:component:coverage | tee /dev/stderr | node_modules/tap-xunit/bin/tap-xunit > component-test-report.xml",
    "lint": "eslint *.js",
    "lint:fix": "eslint --fix *.js"
  },
  "author": "Wealth Wizards",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "6.9.0",
    "babel-eslint": "6.0.4",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-stage-0": "6.5.0",
    "conventional-recommended-bump": "0.3.0",
    "eslint": "2.11.1",
    "eslint-config-airbnb-base": "3.0.1",
    "eslint-plugin-import": "1.8.1",
    "istanbul": "0.4.3",
    "nock": "8.0.0",
    "nodemon": "1.11.0",
    "nsp": "2.6.1",
    "nyc": "8.3.1",
    "proxyquire": "1.7.10",
    "sinon": "1.17.4",
    "sinon-as-promised": "4.0.0",
    "supertest": "1.2.0",
    "tap-spec": "4.1.1",
    "tap-xunit": "1.4.0",
    "tape": "4.6.0",
    "timekeeper": "0.1.1",
    "ww-jenkins-pipeline-utils": "^1.1.1"
  },
  "dependencies": {
    "bcryptjs": "2.3.0",
    "body-parser": "1.15.1",
    "co": "4.6.0",
    "cors": "2.7.1",
    "dotenv": "2.0.0",
    "envalid": "2.2.0",
    "express": "4.14.0",
    "extend": "3.0.0",
    "gridfs-stream": "1.1.1",
    "helmet": "2.1.1",
    "joi": "9.0.0",
    "jsonwebtoken": "7.1.9",
    "lodash": "4.13.1",
    "memorystream": "0.3.1",
    "moment": "2.14.1",
    "mongoose": "4.6.2",
    "mongoose-encryption": "1.3.1",
    "mpath": "0.2.1",
    "ramda": "0.22.1",
    "request": "2.74.0",
    "request-promise-native": "1.0.3",
    "response-time": "2.3.1",
    "underscore": "1.8.3",
    "ww-events-common": "0.0.7",
    "ww-html-to-docx": "0.0.3",
    "ww-logging": "0.3.0",
    "ww-monitoring": "0.13.4",
    "ww-swagger-ui": "2.1.8",
    "ww-utils": "0.11.2"
  }
}
